{
  "name": "tg-blucine-bot",
  "version": "1.0.0",
  "description": "Telegram bot for finding blu-ray release dates",
  "main": "./src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "test": "jest --colors",
    "test-integration": "FETCH_REMOTE=1 jest --colors",
    "test-watch": "jest --watch --colors"
  },
  "engines": {
    "node": ">=10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MiklerGM/tg-blucine-bot.git"
  },
  "author": "Michael Orlov",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/MiklerGM/tg-blucine-bot/issues"
  },
  "homepage": "https://github.com/MiklerGM/tg-blucine-bot#readme",
  "jest": {
    "collectCoverage": true,
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ]
  },
  "lint-staged": {
    "*.js": [
      "eslint",
      "jest --bail --findRelatedTests"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-node": "^4.0.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "husky": "^4.2.3",
    "jest": "^25.2.7",
    "lint-staged": "^10.1.1",
    "nodemon": "^2.0.2",
    "rewire": "^5.0.0",
    "ts-jest": "^25.3.1",
    "ts-node": "^8.8.2"
  },
  "dependencies": {
    "await-to-js": "^2.1.1",
    "axios": "^0.19.2",
    "node-telegram-bot-api": "^0.40.0",
    "node-telegram-keyboard-wrapper": "^2.0.1",
    "sentry": "^0.1.2",
    "typescript": "^3.8.3"
  }
}
